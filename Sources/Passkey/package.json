{
  "name": "Passkey",
  "version": "0.0.0",
  "private": true,
  "repository": "git@github.ibm.com:Langley-Millard/webauthn_carbon.git",
  "bugs": "https://github.ibm.com/Langley-Millard/webauthn_carbon/issues",
  "license": "Apache-2.0",
  "scripts": {
    "build": "react-scripts build",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write",
    "start": "concurrently \"react-scripts start \" \"npm run start:backend\"",
    "start:backend": "cd ../.. && npm start",
    "test": "react-scripts test"
  },
  "options": {
    "allowedHosts": ["localhost", ".localhost"],
    "proxy": "https://localhost:5000/"
  },
  "dependencies": {
    "@carbon/react": "1.1.0",
    "base64url": "^3.0.1",
    "buffer": "^6.0.3",
    "concurrently": "^8.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "react": "16.10.0",
    "react-dom": "16.10.0",
    "react-router-dom": "5.0.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "sass": "1.51.0",
    "wait-for-expect": "3.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
